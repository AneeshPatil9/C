#include <stdio.h>

int main()
{
    int i,j,temp,size;
    int a[10]={9,3,4,6,2,1,5,8,7,10};
    size=10;
    for (i=0;i<size-1;i++)
    {
        for(j=i+1;j<size;j++)
        {
            if(a[i]<a[j])
            {
                temp=a[i];
                a[i]=a[j];
                a[j]=temp;
            }
        }  printf("%d",i);
    }
  
    return 0;
}
